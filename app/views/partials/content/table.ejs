        <h3><%=contentTitle%></h3>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>Package</th>
                  <th>Time Stamp</th>
                  <th>Temperature</th>
                  <th>Humidity</th>
                  <th>Location</th>
                  <th>map</th>
                </tr>
              </thead>
              <tbody>
                <% json.items.forEach(function(item){ %>
                <tr>
                  <td><a href='/api/dynamodbquery/<%= JSON.parse(item.timestp) %>?packageid=<%=  JSON.parse(item.packageId) %>'><%= item.packageId %></a></td>
                  <td><%= item.timestp %></td>
                  <td><%= item.temperature %></td>
                  <td><%= item.humidity %></td>
                  <td><%= item.currLocation %></td>
                  <td><%= item.mapLocation %><%= item.GPS %></td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>


