<% if(jsongps) { %>
    <script>
      var jsongps = <%- JSON.stringify(jsongps) %>;
      function initMap() {
        var uluru = {lat: JSON.parse(jsongps[1]), lng: JSON.parse(jsongps[0])};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 12,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
